.history-list {
	display: flex;
	position: relative;
	flex-direction: column;
	padding: 40rpx;
}
.title {
	font-size: 26rpx;
	color: #8995a1;
}
.item {
	margin-top: 40rpx;
	padding: 15rpx;
	background-color: #fff;
	box-shadow:0 5rpx 10rpx #aaa;
}
.item .query{
	color: #8995a1;
}
.item .result {
	margin-top: 16rpx;
}
.clearItem {
	width: 120rpx;
	height: 120rpx;
	border: 0;
	border-radius: 50%;
	line-height: 120rpx;
	box-shadow:0px 8rpx 30rpx #aaa;
	position: fixed;
	bottom: 60rpx;
	right: 60rpx;
}
.clearItem .icon-cha {
	line-height: 120rpx;
	width: 100%;
	height: 100%;
	font-size: 30rpx;
	color: #fff;
}
